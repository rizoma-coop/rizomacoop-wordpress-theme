{
	"$schema": "https://schemas.wp.org/trunk/theme.json",
	"version": 3,
	"settings": {
		"appearanceTools": true,
		"layout": {
			"contentSize": "700px",
			"wideSize": "1440px"
		},
		"color": {
			"palette": [
				{
					"name": "Primary 50",
					"slug": "primary-50",
					"color": "rgb(30, 129, 127)"
				}
			]
		},
		"typography": {
			"fontFamilies": [
				{
					"name": "Montserrat",
					"fontFamily": "Montserrat, sans-serif",
					"slug": "montserrat"
				}
			],
			"fontSizes": [
				{
					"name": "Large",
					"slug": "large",
					"size": "2rem"
				}
			]
		}
	},
	"styles": {
		"spacing": {
			"blockGap": "2rem"
		},
		"typography": {
			"fontFamily": "var:preset|font-family|montserrat"
		}
	}
}